{
  "title": "Nodejs 入门与进阶",
  "description": "Nodejs 入门与进阶",
  "author": "ruizhengyun <ruizhengyun@gmail.com>",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "links": {
    "sidebar": {
      "我的博客": "http://ruizhengyun.cn/"
    }
  },
  "plugins": [
    "popup",
    "insert-logo",
    "-lunr",
    "-search",
    "search-pro",
    "github",
    "chapter-fold",
    "flexible-alerts",
    "splitter",
    "code",
    "todo",
    "emphasize",
    "-sharing",
    "sharing-plus",
    "auto-scroll-table",
    "custom-favicon",
    "lightbox",
    "anchor-navigation-ex"
  ],
  "pluginsConfig": {
    "favicon": "http://ruizhengyun.cn/favicon.ico",
    "github": {
      "url": "https://github.com/ruizhengyun/nodejs-note"
    },
    "insert-logo": {
      "url": "http://ruizhengyun.cn/static/media/logo.3c2359bf.png",
      "style": "background: none; max-height: 66px; min-height: 66px"
    },
    "sharing": {
      "qq": true,
      "qzone": false,
      "weibo": false,
      "douban": false,
      "all": ["qq", "qzone", "weibo", "douban"]
    },
    "flexible-alerts": {
      "style": "callout",
      "comment": {
        "label": "Comment",
        "icon": "fa fa-comments",
        "className": "info"
      }
    },
    "anchor-navigation-ex": {
      "associatedWithSummary": false,
      "showLevel": true,
      "multipleH1": true,
      "mode": "float",
      "pageTop": {
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      }
    }
  }
}
